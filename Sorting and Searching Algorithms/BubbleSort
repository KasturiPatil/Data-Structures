def BubbleSort(arr):
      for i in range(len(arr)-1):
            for j in range(len(arr)-i-1):
                  if arr[j] > arr[j+1] :
                        arr[j], arr[j+1] = arr[j+1], arr[j]
                        
arr = input("Enter the list of numbers:").split()
arr = [int(x) for x in arr]
BubbleSort(arr)
print(arr)
